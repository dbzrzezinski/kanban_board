{
  "name": "kanban-board",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typescript": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint src --color",
    "format": "prettier --write './src/**/*.{js,jsx,ts,tsx}' --config ./.prettierrc",
